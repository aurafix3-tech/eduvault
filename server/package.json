{
  "name": "eduvault-server",
  "version": "1.0.0",
  "description": "EduVault Backend API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "seed": "node scripts/seedDatabase.js",
    "seed:users": "node scripts/seedUsers.js",
    "seed:institutions": "node scripts/seedInstitutions.js",
    "seed:courses": "node scripts/seedCourses.js",
    "seed:egerton": "node scripts/seedEgertonUniversity.js",
    "seed:egerton-comprehensive": "node scripts/seedEgertonComprehensive.js",
    "seed:all": "npm run seed:institutions && npm run seed:courses && npm run seed:users",
    "setup": "node scripts/setupDatabase.js",
    "create-admin": "node scripts/createTestAdmin.js",
    "create-local-admin": "node scripts/createLocalAdmin.js",
    "update-admin-password": "node scripts/updateAdminPassword.js",
    "debug-admin": "node scripts/debugAdmin.js",
    "fix-admin-password": "node scripts/fixAdminPassword.js",
    "debug-upload": "node scripts/debugUpload.js"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.4",
    "qrcode": "^1.5.4",
    "socket.io": "^4.7.2",
    "stripe": "^13.5.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "education",
    "api",
    "nodejs",
    "mongodb"
  ],
